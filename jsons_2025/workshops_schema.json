{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Workshops Schema",
    "type": "object",
    "properties": {
        "workshops": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "instructor": {
                        "type": "string"
                    },
                    "instructor_designation": {
                        "type": "string"
                    },
                    "instructor_company": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "date": {
                        "type": "string"
                    },
                    "time": {
                        "type": "string"
                    },
                    "venue": {
                        "type": "string"
                    },
                    "modules": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "title": {
                                    "type": "string"
                                },
                                "content": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "title",
                                "content"
                            ]
                        }
                    },
                    "workshop_url": {
                        "type": "string"
                    }
                },
                "required": [
                    "title",
                    "instructor",
                    "instructor_designation",
                    "instructor_company",
                    "description",
                    "date",
                    "time",
                    "venue",
                    "modules",
                    "workshop_url"
                ]
            }
        }
    },
    "required": [
        "workshops"
    ]
}